{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint:write:eslint": "eslint --cache --fix --ext \".js,.vue\" .",
    "lint:write:prettier": "prettier --cache --write .",
    "lint:write": "npm run lint:write:prettier && npm run lint:write:eslint",
    "lint:check:eslint": "eslint --cache --max-warnings=0 --ext \".js,.vue\" .",
    "lint:check:prettier": "prettier --cache --check .",
    "lint:check": "npm run lint:check:prettier && npm run lint:check:eslint"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@types/node": "^18.17.1",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "nuxt": "^3.6.5",
    "prettier": "^3.0.0",
    "vue-eslint-parser": "^9.3.1"
  }
}
